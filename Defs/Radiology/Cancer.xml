<?xml version="1.0" encoding="UTF-8"?>
<Defs>

   <HediffDef Class="Radiology.CancerDef">
      <defName>RadiologyCancer</defName>
      <label>cancer</label>
      
      <defaultLabelColor>(255,66,168)</defaultLabelColor>
      <isBad>true</isBad>
      <scenarioCanAdd>false</scenarioCanAdd>
      
      <likelihood>2.0</likelihood>
      <affectsAllParts>false</affectsAllParts>
      
      <initialSeverityRange><min>0.05</min><max>0.5</max></initialSeverityRange>
      <maxSeverity>1.0</maxSeverity>
      
      <exclusive>cancer</exclusive>

      <spawnEffect>RadiologyEffectCancerMale</spawnEffect>
      <spawnEffectFemale>RadiologyEffectCancerFemale</spawnEffectFemale>
      
   
      <symptomsCount><min>1</min><max>8</max></symptomsCount>
      <symptomsPossible>
        <li>CancerCompPartEfficiency</li>
        <li>CancerCompCapacityOffset</li>
        <li>CancerCompGrowing</li>
        <li>CancerCompGrowingFast</li>
        <li>CancerCompShrinking</li>
        <li>CancerCompDestroyBodyPartAt75</li>
        <li>CancerCompDestroyBodyPartAt100</li>
        <li>CancerCompHunger</li>
        <li>CancerCompMentalBreakDaily</li>
        <li>CancerCompMentalBreakTwoDays</li>
        <li>CancerCompMentalBreakThreeDays</li>
        <li>CancerCompMentalBreakTwiceADay</li>
        <li>CancerCompMentalBreakThriceADay</li>
        <li>CancerCompPainLow</li>
        <li>CancerCompPainMedium</li>
        <li>CancerCompPainHigh</li>
        <li>CancerCompPainExtreme</li>
        <li>CancerCompAggressiveness</li>
        <li>CancerCompVomitingOften</li>
        <li>CancerCompVomitingSometimes</li>
        <li>CancerCompVomitingDaily</li>
        <li>CancerCompDeathAt50</li>
        <li>CancerCompDeathAt75</li>
        <li>CancerCompDeathAt100</li>
        <li>CancerCompSpreadsUpwards</li>
        <li>CancerCompSpreadsDownwards</li>
        <li>CancerCompSpreads</li>
        <li>CancerCompSpreadsMutatesUpwards</li>
        <li>CancerCompSpreadsMutatesDownwards</li>
        <li>CancerCompMutatesSpreads</li>
      </symptomsPossible>
      <diagnoseDifficulty>0.8</diagnoseDifficulty>
      
